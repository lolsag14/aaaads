local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/oShyyyyy/Plaguecheat.cc-Roblox-Ui-library/main/Source.lua", true))()

library:ChangeWeb("discord.gg/nbs")
library:ChangeGame("Praroditel HUB")

local home = library:AddWindow('Home')
local Legit = library:AddWindow('Main')
local aaa = library:AddWindow('Misc')
local watermark = library:AddWatermark('Praroditel hub');


local mainhome = home:AddSection('Info')
local Farm2 = Legit:AddSection('Kill Aura')
local Farm1 = Legit:AddSection('auto collect')
local Farm3 = Legit:AddSection('auto steal')
local aa = aaa:AddSection('Misc')

mainhome:AddLabel('The script by Praroditel" ')
mainhome:AddKeyBind('Toggle GUI', Enum.KeyCode.Insert, function() 
    game.CoreGui.PCR_1.Enabled = not game.CoreGui.PCR_1.Enabled
end)




local hitmark = {
    ["TF2"]       = "rbxassetid://8255306220",
    ["Gamesense"] = "rbxassetid://4817809188",
    ["Rust"]      = "rbxassetid://1255040462",
    ["Neverlose"] = "rbxassetid://8726881116",
    ["Bubble"]    = "rbxassetid://198598793",
    ["Quake"]     = "rbxassetid://1455817260",
    ["Among-Us"]  = "rbxassetid://7227567562",
    ["Ding"]      = "rbxassetid://2868331684",
    ["Minecraft"] = "rbxassetid://6361963422",
    ["Blackout"]  = "rbxassetid://3748776946",
    ["Osu!"]      = "rbxassetid://7151989073",
}


local lox
local selectedHitmarkSoundId = hitmark["Bubble"]

Farm2:AddToggle('kill aura', true, Enum.KeyCode.F, function(state) 
    if state then
        lox = task.spawn(function()
            while true do
                local players = game:GetService("Players")
                local local_player = players.LocalPlayer
                local backpack = local_player.Backpack

                getgenv().settings = {
                    enabled = true,
                    max_hit_distance = clickdelay 
                }

                function get_tool()
                    return local_player.Character and local_player.Character:FindFirstChildOfClass("Tool")
                end

                if settings.enabled then
                    repeat
                        local cute_femboys = {}
                        local tool = get_tool()
                        for _, v in players:GetPlayers() do
                            if v ~= local_player and tool and tool:FindFirstChild("Handle") and v.Character and v.Character.PrimaryPart and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 and (v.Character:GetPivot().Position - local_player.Character:GetPivot().Position).Magnitude < settings.max_hit_distance then
                                table.insert(cute_femboys, v.Character)
                            end
                        end
                        for _, v2 in ipairs(cute_femboys) do 
                            if v2.PrimaryPart then
                                tool:Activate()
                                firetouchinterest(tool.Handle, v2.PrimaryPart, 0)
                                firetouchinterest(tool.Handle, v2.PrimaryPart, 1)

                                local tool = local_player.Backpack:FindFirstChildOfClass("Tool") or local_player.Character:FindFirstChildOfClass("Tool")

                                if tool then
                                    local sound = Instance.new("Sound")
                                    sound.SoundId = selectedHitmarkSoundId
                                    sound.Volume = 4
                                    sound.Looped = false
                                    sound.Parent = tool
                                    sound.Name = "prarod"

                                    sound:Play()

                                    wait(0.2)
                                    sound:Destroy()
                                else
                                    warn("У игрока нет инструментов в инвентаре или в руках.")
                                end     
                            end
                        end
                        task.wait(eatDelay)
                    until not settings.enabled
                end 
            end
        end)
    else
        if lox then 
            task.cancel(lox) 
            lox = nil
        end
    end
end)


Farm2:AddDropdown('Hit markerk sound', {'skit', 'Bubble', 'Neverlose', 'Rust','Osu!','Blackout','Minecraft','Ding','Among-Us','Quake','TF2'}, 'skit', function(selected)
    selectedHitmarkSoundId = hitmark[selected] 
end)


local lox

Farm1:AddToggle('auto collect', true, Enum.KeyCode.Z, function(state) 
    if state then
        lox = task.spawn(function()
            while true do
                wait(0.5)
                local bases = workspace.Bases:GetChildren()

                for _, base in ipairs(bases) do
    
                    local collectZone = base:FindFirstChild("CollectZone")
    
                    if collectZone then
                        collectZone.Size = Vector3.new(500, 2, 500)
                        collectZone.CanCollide = false
                    else
                        warn("eba ti shirni)))")
                    end
                end
            end
        end)
    else
        if lox then 
            task.cancel(lox) 
            lox = nil
        end
    end
end)

local lox

Farm3:AddToggle('auto steal all SC', true, Enum.KeyCode.K, function(state) 
    if state then
        lox = task.spawn(function()
            while true do
                wait(0.2)
                local player = game.Players.LocalPlayer 
                local character = player.Character or player.CharacterAdded:Wait()
                local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
                local hel = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame


                if not humanoidRootPart then
                    warn("HumanoidRootPart не найден")
                    return
                end

                local bases = workspace:FindFirstChild("Bases")
                if bases then
                    for _, base in ipairs(bases:GetChildren()) do
                        if base:IsA("Model") then 
                            print("Найден Base:", base.Name)

                            local slots = base:FindFirstChild("Slots")
                            if slots then
                                for _, slot in ipairs(slots:GetChildren()) do
                                    if slot:IsA("Model") then 
                                        print("Найден Slot:", slot.Name)

                                        for _, model in ipairs(slot:GetChildren()) do
                                            if model:IsA("Model") then 
                                                local hrp = model:FindFirstChild("HumanoidRootPart")
                                                if hrp then

                                                    humanoidRootPart.CFrame = hrp.CFrame
                                                    wait(0.2)
                                                    humanoidRootPart.CFrame = hel
                                                    break 
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end)
    else
        if lox then 
            task.cancel(lox) 
            lox = nil
        end
    end
end)

local lox

Farm1:AddToggle('auto pick up SC', true, Enum.KeyCode.C, function(state) 
    if state then
        lox = task.spawn(function()
            while true do
                wait(0.5)
                local player = game.Players.LocalPlayer
                local character = player.Character or player.CharacterAdded:Wait()
                local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
                local range = 50

                for _, descendant in ipairs(workspace:GetDescendants()) do
                    if descendant:IsA("ProximityPrompt") then
                        local distance = (descendant.Parent.Position - humanoidRootPart.Position).magnitude
                        if distance <= range then
                            if fireproximityprompt then
                                fireproximityprompt(descendant)
                            else
                                warn("zozi.")
                            end
                        end
                    end
                end
            end
        end)
    else
        if lox then 
            task.cancel(lox) 
            lox = nil
        end
    end
end)


local lox

Farm1:AddToggle('prompt cooldown 0', true, Enum.KeyCode.X, function(state) 
    if state then
        lox = task.spawn(function()
            while true do
                wait(0.2)
                for i,v in ipairs(game:GetService("Workspace"):GetDescendants()) do
                    if v.ClassName == "ProximityPrompt" then
                        v.HoldDuration = 0
                    end
                end
            end
        end)
    else
        if lox then 
            task.cancel(lox) 
            lox = nil
        end
    end
end)

local lox

Farm1:AddToggle('door destroy', true, Enum.KeyCode.P, function(state) 
    if state then
        lox = task.spawn(function()
            while true do
                wait(0.5)
                for _, base in pairs(workspace.Bases:GetChildren()) do
                    if base:FindFirstChild("Doors") then
                        local door = base.Doors:FindFirstChild("Door1")
                        if door then
                            door:Destroy()
                        end
                    end
                end
            end
        end)
    else
        if lox then 
            task.cancel(lox) 
            lox = nil
        end
    end
end)

clickdelay=5

Farm2:AddSlider('kill aura distance', 40, 1, 5,function(value1) 
    clickdelay=value1
end)


eatDelay=0.1

Farm2:AddSlider('kill aura delay', 10, 0.1, 0.1,function(value1) 
    eatDelay=value1
end)

aa:AddSlider('localplayer Speed', 100, 1, 28,function(Value) 
    getgenv().WalkSpeedValue = (Value);
    local Player = game:service'Players'.LocalPlayer;
    Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
    Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
    end)
    Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
end)


